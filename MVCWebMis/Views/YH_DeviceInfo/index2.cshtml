                         <th>医院地址</th>
                         <th>医院邮编</th>
                         <th>医院电话</th>
                         <th>传真号码</th>
                         <th>主联系人</th>
                         <th>联系人电话</th>
                         <th>联系人手机</th>
                         <th>电子邮件</th>
                         <th>客户类别</th>
                         <th>客户级别</th>
                         <th>信用等级</th>
                         <th>重要级别</th>
                         <th>备注信息</th>
                             <th>设备ID</th>
                          <th>经度</th>
                         <th>纬度</th>
                         <th>仪器在线状态</th>
  
                        <th style="width:120px">操作</th>
                    </tr>
                </thead>
                <tbody id="grid_body"></tbody>
            </table>
            <div class="paging-toolbar">
                <ul id='grid_paging'></ul>
            </div>
        </div>
    </div>
</div>


