    <script>
        function getUrlVars() {
            var vars = [], hash;
            var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
            for (var i = 0; i < hashes.length; i++) {
                hash = hashes[i].split('=');
                vars.push(hash[0]);
                vars[hash[0]] = hash[1];
            }
            return vars;
        }

        //修改用户密码
        function showPassword() {
            $("#password").modal("show");
        }
        function changePassword() {
            // if ($("#txtNewPass").val() == "") {
            //     $("#txtNewPass").focus();
            //     showToast('新密码不能为空', "warning");
            //      return;
            // }
            if ($("#txtNewPass").val() != $("#txtCheckPass").val()) {
                $("#txtNewPass").focus();
                showToast("两次输入的密码不一致", "warning");
                return;
            }

            var postData = { Name: '@Session["Identity"]', OldPass: $("#txtOldPass").val(), NewPass: $("#txtNewPass").val() };
            $.ajax({
                type: 'POST',
                url: '/User/ModifyPass?r=' + Math.random(),
                data: postData,
                dataType: 'json',
                success: function (data) {
                    if (data.Success) {
                        showToast("操作成功");

                        $("#password").modal('hide');
                    }
                    else {
                        showToast("操作失败：" + data.ErrorMessage, 3000);
                    }
                }
            });
        }

        //设置Cookie后跳转到锁定页面
        function lock() {
            //先主动清空用户Session数据
            var url = '/Login/ClearSession?r=' + Math.random();
            var postData = null
            $.post(url, postData, function (json) {
            });

            //使用{ path: "/"} 解决Cokie不同页面取值问题
            Cookies.set("lock_UserID", '@Session["UserID"]', { path: "/" });
            Cookies.set("lock_FullName", '@Session["FullName"]', { path: "/" });
            Cookies.set("lock_UserName", '@Session["Identity"]', { path: "/" });
            Cookies.set("lock_UserID2", '@Session["UserID"]', { path: "/" });

            window.location.href = '/Login/Lock';
        };

